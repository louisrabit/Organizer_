 @page "/Personal"
@inject NavigationManager NavManager

<MudContainer Class="mt-16 px-8" MaxWidth="MaxWidth.False">
    <MudGrid>
        <MudItem xs="12" sm="6" md="6">
            <MudPaper Elevation="2" Class="pa-4 mud-theme-primary border-5" Style="height: 200px; border-radius:12px">
                <MudText Class="align-self-center py-4 flex-1 " Typo="Typo.h6" Align="Align.Center">
                    Today Tickets
                </MudText>
                <MudText Typo="Typo.body2" Align="Align.Center" Class="mt-5">
                    Number Tickets Today:
                </MudText>
            </MudPaper>
        </MudItem>

        
    </MudGrid>
</MudContainer>
 